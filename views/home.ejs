<%- include('partials/header')  %> 

    <h1>Home</h1>
    <p> <%= startingContent %> </p>
<% Posts.forEach(function(post){            %>
    <h1><%= post.title;  %></h1>
    <p>
        <%= post.content.substring(0,100)+ "..." %>
        <% const link = "/posts/"+post.title %>
        <a href="<%= link %>">Read More</a>
    </p>
<% });                                      %>


    
<%- include('partials/footer') %>